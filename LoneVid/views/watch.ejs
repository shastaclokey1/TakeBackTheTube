<% include partials/header %>
<div class="text-center">
	<h1>Watch</h1>
</div>


<div class="text-center" id="GrowAndShrinkWithPageWidth">
	<iframe width="420" height="315" src="https://www.youtube.com/embed/<%=videoDetails.videoId%>"
		allowfullscreen="allowfullscreen"
		mozallowfullscreen="mozallowfullscreen" 
		msallowfullscreen="msallowfullscreen" 
		oallowfullscreen="oallowfullscreen" 
		webkitallowfullscreen="webkitallowfullscreen">
	</iframe>
</div>

<div class="text-center">
	<h2 id="VideoTitle"><%=videoDetails.title%></h2>
	<p id="VideoTimestamp"><%=videoDetails.timestamp%></p>
	<p id="VideoDescription"><%=videoDetails.description%></p>
</div>
<% include partials/footer %>
